{
    "name": "posthog-node",
    "version": "1.3.2",
    "description": "PostHog Node.js integration",
    "license": "MIT",
    "repository": "PostHog/posthog-node",
    "author": {
        "name": "PostHog",
        "email": "hey@posthog.com",
        "url": "https://posthog.com"
    },
    "engines": {
        "node": ">=14"
    },
    "size-limit": [
        {
            "limit": "25 KB",
            "path": "index.js"
        }
    ],
    "scripts": {
        "dependencies": "yarn",
        "size": "size-limit",
        "test": "jest --forceExit",
        "test:watch": "jest --watch",
        "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
        "format": "prettier --write ."
    },
    "files": [
        "index.js",
        "index.d.ts",
        "fetchRetry.js",
        "fetchTimeout.js",
        "decoratedFetch.js",
        "event-validation.js",
        "cli.js",
        "feature-flags.js"
    ],
    "bin": {
        "posthog": "cli.js"
    },
    "keywords": [
        "posthog",
        "stats",
        "analysis",
        "funnels"
    ],
    "dependencies": {
        "abort-controller": "^3.0.0",
        "async-retry": "^1.3.3",
        "component-type": "^1.2.1",
        "debug": "^4.3.3",
        "is-retry-allowed": "2.2.0",
        "join-component": "^1.1.0",
        "md5": "^2.3.0",
        "ms": "^2.1.3",
        "remove-trailing-slash": "^0.1.1",
        "undici": "^4.13.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "ava": "^3.15.0",
        "basic-auth": "^2.0.1",
        "body-parser": "^1.19.1",
        "codecov": "^3.8.3",
        "commander": "^2.20.3",
        "delay": "^5.0.0",
        "express": "^4.17.2",
        "jest": "^27.5.1",
        "msw": "^0.36.8",
        "nyc": "^15.1.0",
        "pify": "^5.0.0",
        "prettier": "^2.5.1",
        "sinon": "^7.5.0",
        "size-limit": "^7.0.8",
        "snyk": "^1.851.0"
    }
}
